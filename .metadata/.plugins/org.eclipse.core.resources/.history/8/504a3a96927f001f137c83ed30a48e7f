package com.korea.user.dto;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Builder
@NoArgsConstructor
@AllArgsConstructor
@Data
public class UserDTO {
	
	private int id;
	private String name;	
	private String email;
	
	public UserDTO(UserEntity entity) {
		this.id = id;
		this.name = name;
		this.email.email;
		
	}
}
